{
  "name": "alarmasdelsubte",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development ts-node-dev --respawn --transpileOnly src/app.ts",
    "prod": "tsc && cross-env NODE_ENV=production forever start ./build/src/app.js",
    "pretest": "eslint --fix --max-warnings 0 src/**/*.ts tests/**/*.ts",
    "test": "cross-env NODE_ENV=test nyc mocha -r ts-node/register --colors --file tests/setup.ts 'tests/**/*.ts'",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "migrations:generate": "ts-node node_modules\\typeorm\\cli.js migration:generate"
  },
  "dependencies": {
    "@types/config": "0.0.36",
    "@types/express": "^4.17.2",
    "@types/graphql": "^14.5.0",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.1.4",
    "axios": "^0.19.2",
    "class-validator": "^0.11.0",
    "config": "^3.3.8",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-graphql": "^0.9.0",
    "firebase-admin": "^8.9.2",
    "forever": "^2.0.0",
    "graphql": "^14.5.8",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.1.1",
    "ts-md5": "^1.2.7",
    "ts-node-dev": "^1.0.0-pre.44",
    "ts-token-generator": "^1.0.4",
    "type-graphql": "^0.17.6",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.22",
    "typeorm-typedi-extensions": "^0.2.3",
    "typescript": "^3.7.4",
    "typescript-json-serializer": "^1.4.2",
    "typescript-string-operations": "^1.3.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/chai-as-promised": "^7.1.2",
    "@types/mocha": "^5.2.7",
    "@types/mock-req-res": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.9",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "mocha": "^6.2.2",
    "mock-req-res": "^1.1.4",
    "nyc": "^15.0.0",
    "sinon": "^8.1.1",
    "sinon-chai": "^3.4.0",
    "ts-mockito": "^2.5.0",
    "ts-node": "^6.2.0",
    "ts-sinon": "^1.0.24"
  },
  "nyc": {
    "exclude": [
      "config/**",
      "src/db/migration/**",
      "tests/**"
    ]
  }
}
